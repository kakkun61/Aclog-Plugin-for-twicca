            android:name=".ShowTweetActivity"
            android:label="@string/open_aclog" >
            <intent-filter>
                <action android:name="jp.r246.twicca.ACTION_SHOW_TWEET" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
